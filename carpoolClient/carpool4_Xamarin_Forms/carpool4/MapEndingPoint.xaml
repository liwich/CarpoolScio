<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Carpool.MapEndingPoint"
             Title="Select Ending point">
  <StackLayout x:Name="stackMap" VerticalOptions="FillAndExpand" Style="{StaticResource contentPageStyle}" Padding="0,10,0,0">
    <Label Style="{StaticResource labelBoldStyle}" x:Name="infoLabel" HorizontalTextAlignment="Center"> Loading map and location...</Label>
  </StackLayout>
</ContentPage>